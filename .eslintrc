{
  "globals": {
    "jQuery": true,
    "$": true,
    "describe": false,
    "it": false,
    "expect": false,
    "$on": false,
    "beforeEach": false
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "airbnb/base",
  "rules": {
    "indent": 0,
    "func-names": 0,
    "no-var": 0,
    "func-style": 0,
    "comma-dangle": 0,
    "valid-jsdoc": 0,
    "vars-on-top": 1,
    "complexity": [2, 8],
    "max-depth": [2, 6],
    "eol-last": 0,
    "wrap-iife": [2, "any"],
    "no-multi-spaces": 1,
    "space-before-function-paren": 1,
    "key-spacing": 1,
    "consistent-return": 0,
    "id-match": 0,
    "import/newline-after-import": 0,
    "one-var": [2, "always"],
    "strict": 0,
    "space-in-parens": 1,
    "computed-property-spacing":1,
    "max-len": 1,
    "quote-props": 1,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0,
    "prefer-template": 0,
    "babel/object-shorthand": 0
  }
}