{
  "name": "Queensland-Government-Web-Template",
  "version": "3.1.9",
  "subVersion": "1.0.9",
  "wt-version": "3.1.9",
  "description": "This template is designed to provide a template for all Franchise websites, and the underpinning technology for new Agency websites.",
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/qld-gov-au/qg-web-template.git"
  },
  "scripts": {
    "clean": "gulp clean-build && gulp clean-release",
    "prebuild": "gulp clean-build",
    "build": "gulp build && node gulp/build-tasks/node-templates-flatten.js && gulp wt-updateVersion && node gulp/release-tasks/node-template-flatten.js",
    "test": "jest",
    "postbuild": "gulp clean-redundant-build",
    "prerelease": "gulp clean-release",
    "release": "export NODE_ENV=prod && npm run build && gulp release && node gulp/release-tasks/node-template-flatten.js && gulp replace-links",
    "postrelease": "gulp clean-redundant-release",
    "start": "yarn build && gulp watch",
    "watch": "gulp watch",
    "publish-npm": "gulp wt-clean && gulp wt-clone && gulp wt-sync && gulp wt-updateVersion && gulp wt-npm && gulp wt-add && gulp wt-commit && gulp wt-tag && gulp wt-push",
    "create-swe-release": "gulp swe-add && gulp swe-tag && gulp swe-commit && gulp swe-push",
    "publish-cdn": "gulp cdn-clean && gulp cdn-clone && gulp cdn-transfer && gulp cdn-add && gulp cdn-commit && gulp cdn-push",
    "prepublish": "npm run release",
    "publish-npm-cdn": "npm run publish-npm && npm run publish-cdn",
    "postpublish": "gulp clean-publish"
  },
  "authors": "Queensland Government",
  "licence": "all rights reserved",
  "homepage": "http://www.qld.gov.au/",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "debug": "^4.1.1",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-semistandard": "^15.0.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-html-reporter": "^0.7.4",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "event-stream": "^4.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^5.1.0",
    "font-awesome": "^4.7.0",
    "fs": "0.0.1-security",
    "fs-path": "0.0.24",
    "git-hooks": "^1.1.10",
    "gulp": "^4.0.2",
    "gulp-add-src": "^1.0.0",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-connect-ssi": "^0.4.0",
    "gulp-directory-sync": "^1.2.4",
    "gulp-eslint": "^6.0.0",
    "gulp-git": "^2.10.0",
    "gulp-if": "^3.0.0",
    "gulp-include": "^2.4.1",
    "gulp-load-plugins": "^2.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-prompt": "^1.2.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "historyjs": "^1.8.0-b2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-html-reporter": "^2.8.0",
    "jest-junit": "^10.0.0",
    "node-sass": "^4.13.1",
    "node-ssi": "^0.3.2",
    "npm": ">=6.14.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "vinyl-fs": "^3.0.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-replace": "^1.0.0",
    "webpack-stream": "^5.2.1",
    "yargs": "^15.1.0"
  },
  "dependencies": {
    "bootstrap": "^4.4.1",
    "bootstrap-accessibility-plugin": "^1.0.6",
    "breakpoint-sass": "^2.7.1",
    "chromium": "^2.1.1",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "gulp-foreach": "^0.1.0",
    "gulp-insert": "^0.5.0",
    "gulp-postcss": "^8.0.0",
    "gulp-shell": "^0.8.0",
    "jquery": "^3.4.1",
    "lato-webfont": "^2.15.1",
    "node-bourbon": "^4.2.8",
    "puppeteer-core": "^2.1.1",
    "regenerator-runtime": "^0.13.3",
    "replace-string": "^3.0.0",
    "responsive-toolkit": "^2.6.3",
    "stickyfilljs": "^2.1.0"
  },
  "jest-junit": {
    "suiteName": "SWE tests",
    "outputDirectory": "./test-results/junit/junit.xml"
  }
}
